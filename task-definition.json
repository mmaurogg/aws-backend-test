{
  "family": "api-facturas-task-definition",
  "networkMode": "awsvpc",
  "cpu": "256",   
  "memory": "512", 
  "executionRoleArn": "arn:aws:iam::932552648067:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::932552648067:role/api-facturas-task-role",
  "containerDefinitions": [
    {
      "name": "api-facturas-container", 
      "image": "932552648067.dkr.ecr.us-east-2.amazonaws.com/api-facturas:lastest", 
      "portMappings": [
        {
          "containerPort": 8080, 
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/api-facturas",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "runtimePlatform": {
      "operatingSystemFamily": "LINUX"
  }
}
