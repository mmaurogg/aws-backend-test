{
  "family": "api-facturas-task-definition",
  "networkMode": "awsvpc",
  "cpu": "256",   
  "memory": "512", 
  "executionRoleArn": "arn:aws:iam::041247148008:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::041247148008:role/api-facturas-task-role",
  "containerDefinitions": [
    {
      "name": "api-facturas-container", 
      "image": "041247148008.dkr.ecr.us-east-1.amazonaws.com/api-facturas:12f05ca", 
      "portMappings": [
        {
          "containerPort": 8080, 
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/api-facturas", 
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "runtimePlatform": {
      "operatingSystemFamily": "LINUX"
  }
}
